<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".registro_cliente3">
    <TextView
            android:text="Preguntas de Seguridad"
            android:layout_width="219dp"
            android:layout_height="18dp"
            android:id="@+id/label_preguntasseguridadcliente"
            android:clickable="false" android:background="#FBFBFB"
            android:textColor="#009688"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="60dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"/>
    <TextView
            android:text="Pregunta 1"
            android:layout_width="81dp"
            android:layout_height="26dp"
            android:id="@+id/label_pregunta1_cliente" android:textColor="#009688"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginTop="100dp"/>
    <TextView
            android:text="Respuesta 1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/label_respuesta1_cliente" android:textColor="#3F51B5"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/pregunta1_cliente"
            app:layout_constraintStart_toStartOf="@+id/pregunta1_cliente"
    />
    <EditText
            android:layout_width="366dp"
            android:layout_height="49dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/respuesta1_cliente"
            android:textColor="#FF9800"
            app:layout_constraintTop_toBottomOf="@+id/label_respuesta1_cliente"
            app:layout_constraintStart_toStartOf="@+id/label_respuesta1_cliente"
    />
    <TextView
            android:text="Pregunta 2"
            android:layout_width="83dp"
            android:layout_height="25dp"
            android:id="@+id/label_pregunta2_cliente" android:textColor="#009688"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/respuesta1_cliente"
            app:layout_constraintStart_toStartOf="@+id/respuesta1_cliente"/>
    <TextView
            android:text="Respuesta 2"
            android:layout_width="122dp"
            android:layout_height="27dp"
            android:id="@+id/label_respuesta2_cliente"
            android:textColor="#3F51B5"
            android:layout_marginTop="39dp"
            app:layout_constraintTop_toBottomOf="@+id/pregunta2_cliente"
            app:layout_constraintStart_toStartOf="@+id/pregunta2_cliente"/>
    <EditText
            android:layout_width="366dp"
            android:layout_height="49dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/respuesta2_cliente" android:textColor="#FF9800"
            app:layout_constraintTop_toBottomOf="@+id/label_respuesta2_cliente"
            app:layout_constraintStart_toStartOf="@+id/label_respuesta2_cliente"/>
    <Button
            android:text="Continuar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/cont_regcliente2" android:background="#FBFBFB"
            android:textColor="#009688"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="AtrÃ¡s"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/atras_registrocliente2" android:textColor="#F44336"
            android:background="#FBFBFB"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginBottom="16dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/primer_nombre"
            android:visibility="invisible"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="parent" app:layout_constraintBottom_toTopOf="parent"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="39dp"
            android:id="@+id/segundo_nombre" android:visibility="invisible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/primer_nombre"
            app:layout_constraintEnd_toStartOf="@+id/segundo_apellido"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/primer_apellido"
            android:visibility="invisible"
            android:layout_marginTop="1dp"
            app:layout_constraintStart_toEndOf="@+id/email_cliente" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="1dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/segundo_nombre" android:layout_marginEnd="411dp"
            android:layout_marginRight="411dp"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="39dp"
            android:id="@+id/segundo_apellido"
            android:visibility="invisible"
            app:layout_constraintBaseline_toBaselineOf="@+id/primer_nombre"
            app:layout_constraintStart_toEndOf="@+id/segundo_nombre"
            app:layout_constraintEnd_toStartOf="@+id/tipo_doccliente"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="22dp"
            android:id="@+id/movil_clientecompleto"
            android:visibility="invisible"
            app:layout_constraintStart_toStartOf="@+id/respuesta2_cliente" android:layout_marginTop="34dp"
            app:layout_constraintTop_toBottomOf="@+id/respuesta2_cliente"
            app:layout_constraintEnd_toStartOf="@+id/respuesta2_cliente"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:id="@+id/tipo_doccliente" android:visibility="invisible"
            app:layout_constraintBaseline_toBaselineOf="@+id/primer_nombre"
            app:layout_constraintStart_toEndOf="@+id/segundo_apellido"
            app:layout_constraintEnd_toStartOf="@+id/numdoc_cliente"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:id="@+id/numdoc_cliente"
            android:visibility="invisible"
            app:layout_constraintBaseline_toBaselineOf="@+id/primer_nombre"
            app:layout_constraintStart_toEndOf="@+id/tipo_doccliente"
            app:layout_constraintEnd_toStartOf="@+id/email_cliente"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:id="@+id/email_cliente"
            android:visibility="invisible"
            app:layout_constraintBaseline_toBaselineOf="@+id/primer_nombre"
            app:layout_constraintStart_toEndOf="@+id/numdoc_cliente"
            app:layout_constraintEnd_toStartOf="@+id/primer_apellido"/>
    <Spinner
            android:layout_width="370dp"
            android:layout_height="36dp"
            android:id="@+id/pregunta1_cliente"
            android:entries="@array/pregunta1_cliente"
            app:layout_constraintTop_toBottomOf="@+id/label_pregunta1_cliente"
            app:layout_constraintStart_toStartOf="@+id/label_pregunta1_cliente"
    />
    <Spinner
            android:layout_width="368dp"
            android:layout_height="32dp"
            android:entries="@array/pregunta2_cliente"
            android:id="@+id/pregunta2_cliente"
            app:layout_constraintTop_toBottomOf="@+id/label_pregunta2_cliente"
            app:layout_constraintStart_toStartOf="@+id/label_pregunta2_cliente"/>

</androidx.constraintlayout.widget.ConstraintLayout>